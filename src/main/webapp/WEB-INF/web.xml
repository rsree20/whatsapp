<web-app>
	<servlet>
		<servlet-name>whatsapp</servlet-name>
		<servlet-class>whatsapp.WhatsappServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>whatsapp</servlet-name>
		<url-pattern>/Loginpage</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>whatsapp.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>whatsapp.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>WhatsappLogin.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ForgotServlet</servlet-name>
		<servlet-class>whatsapp.ForgotServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ForgotServlet</servlet-name>
		<url-pattern>/ForgotPassword</url-pattern>
	</servlet-mapping>
		<welcome-file-list>
		<welcome-file>ForgotPassword.jsp</welcome-file>
	</welcome-file-list>
	</web-app>
